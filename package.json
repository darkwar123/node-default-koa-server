{
  "name": "default-koa-server",
  "version": "1.0.0",
  "description": "Default Koa.js server",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darkwar123/node-default-koa-server.git"
  },
  "scripts": {
    "dev": "set NODE_ENV=development && set DEBUG=* && set PUBLIC_DIR=public && node index.js",
    "prod": "set NODE_ENV=production && set PORT=3000 && set BASE_URL=http://localhost:3000 && set PUBLIC_DIR=public && set DEBUG=*:info,*:error && node index.js"
  },
  "keywords": [
    "koa.js",
    "koa",
    "http2"
  ],
  "author": "darkwar123",
  "license": "ISC",
  "dependencies": {
    "debug": "^2.5.2",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-bunyan": "^1.0.1",
    "koa-helmet": "^4.0.0",
    "koa-passport": "^4.1.0",
    "koa-qs": "^2.0.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-session": "^5.8.1",
    "koa-static": "^5.0.0"
  }
}
